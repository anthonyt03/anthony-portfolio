    "@tsparticles/slim": "^3.7.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cobe": "^0.6.3",
    "dotted-map": "^2.2.3",
    "framer-motion": "^11.14.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.462.0",
    "mini-svg-data-uri": "^1.4.4",
    "motion": "^11.12.0",
    "next": "^15.0.1",
