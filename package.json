    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^9.118.0",
    "@react-three/fiber": "^8.17.10",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tabler/icons-react": "^3.24.0",
